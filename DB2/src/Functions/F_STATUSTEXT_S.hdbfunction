FUNCTION "F_STATUSTEXT_S"( IV_ID INTEGER, IV_LANGU VARCHAR(2) )
       RETURNS EV_STATUS_TEXT NVARCHAR(20) 
       LANGUAGE SQLSCRIPT 
       SQL SECURITY INVOKER AS 
BEGIN 

 SELECT "STATUS_TEXT" into EV_STATUS_TEXT
	FROM "STATUS_TEXT" 
    WHERE "ID" = :IV_ID and  "LANGU" = IV_LANGU;
    
END;
