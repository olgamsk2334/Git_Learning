FUNCTION "F_USER_NAME_T"( )
       RETURNS TABLE (
    			 	"USERNAME" NVARCHAR(60),
    			 	"TITLE"    NVARCHAR(40)
       )
       LANGUAGE SQLSCRIPT 
       SQL SECURITY INVOKER AS 
BEGIN 
    RETURN SELECT "U"."FIRSTNAME" || ' ' || "U"."LASTNAME"	AS "USERNAME", "P"."TITLE"
	FROM (
		SELECT *
		FROM "PROJECTS"
		where "TITLE" LIKE 'F%'
	) AS "P"
		INNER JOIN  "USERS" AS "U" ON
		"P"."PROJECT_MANAGER" = "U"."ID";
END;