FUNCTION "F_TASK_IN_STATUS_T"(
			IV_STATUS INTEGER
	)
    RETURNS TABLE (
       		"TITLE" NVARCHAR (40),
       		"ASSIGNEE" INTEGER,
       		"CREATE_DATE" DATE
       ) 
       LANGUAGE SQLSCRIPT 
       SQL SECURITY INVOKER AS 
BEGIN 
 RETURN SELECT "TITLE",
				"ASSIGNEE",
				"CREATE_DATE"
		FROM "TASKS"
		WHERE "STATUS" = :IV_STATUS;
END;