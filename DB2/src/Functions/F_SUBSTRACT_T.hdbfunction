FUNCTION "F_SUBSTRACT_T"( )
       RETURNS TABLE( "ASSIGNEE" INTEGER
       	)
       LANGUAGE SQLSCRIPT 
       SQL SECURITY INVOKER AS 
BEGIN 
    RETURN SELECT DISTINCT "ASSIGNEE"
    		FROM "TASKS" AS "T"
    	--	EXCEPT
    	--	SELECT "PROJECT_MANAGER"
    	--	FROM "PROJECTS";
    	WHERE NOT EXISTS(
    	SELECT	"PROJECT_MANAGER"
    	FROM "PROJECTS" as "P"
    	WHERE P."PROJECT_MANAGER" = "T"."ASSIGNEE"
    	);
END;