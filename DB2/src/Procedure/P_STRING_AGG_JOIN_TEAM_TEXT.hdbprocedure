PROCEDURE "P_STRING_AGG_JOIN_TEAM_TEXT"( 
	OUT ET_RESULT TABLE (
			             "TEAM_TEXT" NVARCHAR(200)))
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
	ET_RESULT = SELECT STRING_AGG("TEAM_TEXT"., ' , ') AS "TEAM_TEXT"
   
END