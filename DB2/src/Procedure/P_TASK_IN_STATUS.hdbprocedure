PROCEDURE "P_TASK_IN_STATUS"(
		IN	IV_STATUS INTEGER DEFAULT '3',
		OUT ET_TASKS TABLE (
				"TITLE" NVARCHAR (40),
       			"ASSIGNEE" INTEGER,
       			"CREATE_DATE" DATE
		)
)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
 ET_TASKS = SELECT "TITLE",
				"ASSIGNEE",
				"CREATE_DATE"
			FROM "TASKS"
			WHERE "STATUS" = :IV_STATUS;
END