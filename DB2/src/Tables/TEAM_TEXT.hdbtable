COLUMN TABLE "TEAM_TEXT"(
	"ID"				INTEGER CS_INT	NOT NULL 	COMMENT 'Internal Identifier',
	"LANGU"				NVARCHAR(2)		NOT NULL	COMMENT 'Language',
	"TEAM_TEXT"			NVARCHAR(20)    			COMMENT '',

	PRIMARY KEY ("ID", "LANGU")
	
) WITH ASSOCIATIONS( 
	JOIN "TASKS" AS "T" ON "T"."ID" = "ID"
)

UNLOAD PRIORITY 5 AUTO MERGE
COMMENT 'Team text table';
	