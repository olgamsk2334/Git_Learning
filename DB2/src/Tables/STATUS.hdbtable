COLUMN TABLE "STATUS"(
	"ID"				INTEGER CS_INT	NOT NULL 	COMMENT 'Internal Identifier',
	"IS_FINAL"			BOOLEAN						COMMENT '',
	"IS_OPEN"			BOOLEAN    					COMMENT '',

	PRIMARY KEY ("ID")
	
) WITH ASSOCIATIONS( 
	JOIN "TASKS" AS "T" ON "T"."ID" = "ID"
)

UNLOAD PRIORITY 5 AUTO MERGE
COMMENT 'Status table';
	